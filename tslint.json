{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-standard"],

  "rules": {
      "max-line-length": false,
      "new-parens": true,
      "no-arg": true,
      "no-bitwise": true,
      "no-conditional-assignment": true,
      "no-consecutive-blank-lines": false,
      "no-console": {
          "severity": "warning",
          "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
      },
      "interface-name": false,
      "jsx-no-multiline-js": false
  },
  "jsRules": {

  }
}
